generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("AIVEN_DB_URL")
}

model DanceEvent {
  id          String   @id @default(uuid())
  name        String
  style       String[]
  date        DateTime
  country     String
  city        String
  venue       Json[]
  description Json
  sns         Json[]
  flyer       Json
}
