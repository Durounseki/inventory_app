<h3>Test</h3>
<div class="event">
    <h2 class="event-name">
        <%= event.name%>
    </h2>
    <h3 class="headline">
        <%= event.description.headline %>
    </h3>
    <p class="description">
        <%= event.description.body %>
    </p>
    <p class="cta">
        <%= event.description.cta %>
    </p>
    <div class="details">
        <p class="date">
            <i class="fa-regular fa-calendar"></i><%= dayjs(event.date).format('YYYY MMMM D') %>
        </p>
        <p class="venue">
            <i class="fa-regular fa-map"></i><% if(event.venue.url){ %><a href="<%= event.venue.url %>"><%= event.venue.name %></a><% } else {%><%= event.venue.name %><% } %>
        </p>
        <p class="sns">
            <% for(sns of event.sns) { %>
                <a href="<%= sns.url %>" aria-label="<%= sns.name %>"><i class="<%= sns.faClass %>"></i></a>
            <% } %>
        </p>
    </div>

</div>